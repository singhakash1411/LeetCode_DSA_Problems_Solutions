class Solution {
public:
    string largestGoodInteger(string num) {
       char digit =0;
       for(int i =0; i+2 < num.size(); i++){
        if(num[i] == num[i +1] && num[i] == num[i+2]){
            if(num[i] > digit){
                digit = num[i];
            }
        }
       }
       if(digit == 0)return "";
       return string(3, digit);
        
    }
};
